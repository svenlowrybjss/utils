{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{if .Root}} ‚ö° {{end}}{{.User}} ‚ûî üìÅ{{.Folder}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "display_host": false,
            "wsl": "",
            "wsl_separator": ""
          },
          "template": "\u2800{{.Icon}}",
          "foreground": "#100e23",
          "background": "#ffffff",
          "type": "os",
          "style": "powerline",
          "background_templates": [
            "{{if .Root}}#ff3026{{else}}#cfcfc4{{end}}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "15:04:05"
          },
          "foreground": "#100e23",
          "powerline_symbol": "\ue0b0",
          "background": "#9e9e9a",
          "type": "time",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "\uf115",
            "folder_separator_icon": "\u2800\ue0b1\u2800\ue5ff\u2800",
            "home_icon": "\uf015",
            "max_depth": 1,
            "style": "agnoster_short"
          },
          "template": "\u2800{{ .Path }}\u2800",
          "foreground": "#100e23",
          "powerline_symbol": "\ue0b0",
          "type": "path",
          "style": "powerline",
          "background_templates": [
            "{{ if eq .OS \"ubuntu\" }}#ec9706{{ end }}",
            "{{ if eq .OS \"windows\" }}#0079D8{{ end }}",
            "#2f2f2f"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf0c7 {{ .StashCount }}{{ end }} ",
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "background": "#ffeb3b",
          "type": "git",
          "style": "powerline",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FFEB3B{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFCC80{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FB{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "command": "[[ ! -z ${AWS_VAULT+z} ]] && echo \"$SHLVL\" || echo 0",
            "shell": "zsh"
          },
          "template": "{{ if ( gt (.Output | int) 0 ) }}\u2800\ue683\u2800{{ end }}",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#cfcfc4",
          "type": "command",
          "style": "powerline"
        }
      ]
    },
    {
      "type": "rprompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0c2",
          "trailing_diamond": "\ue0c0",
          "template": " \ue70f ",
          "foreground": "#ffffff",
          "background": "#ff4040",
          "type": "exit",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "austin"
          },
          "leading_diamond": "\ue0b2",
          "template": " {{ .FormattedMs }} ",
          "foreground": "#fafafa",
          "background": "#2f2f2f",
          "type": "executiontime",
          "style": "diamond"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
